<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    {{#if css}}
      <style>
        {{{ css }}}
      </style>
    {{else}}
      <link rel="stylesheet" href="main.css">
    {{/if}}
  </head>
  <body>
    <div class="game">
      <table>
        <tr>
          <td>
            <div class="s sBob" id="sBob"></div>
          </td>
          <td rowspan="2" class="mapWrap">
            <table class="map" id="map"></table>
          </td>
          <td>
            <div class="s sTom" id="sTom"></div>
          </td>
        </tr>
        <tr>
          <td>
            <div class="s sRon" id="sRon"></div>
          </td>
          <td>
            <div class="s sMax" id="sMax"></div>
          </td>
        </tr>
      </table>
      <button id="nextTurn">Next Turn</button>
    </div>
    {{#if js}}
      <script>
        {{{ js }}}
      </script>
    {{else}}
      <script src='main.js'></script>
    {{/if}}
  </body>
</html>
